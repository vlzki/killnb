


<div class="col-sm-6">
<div class="block">
<form>
	<%= simple_form_for @job, url: assets_job_path(@asset) do |f| %>
	<%= f.error_notification %>
		<div class="form-group">
			<%= f.input :target_name %>
		</div>
		<div class="form-group">
			<%= f.input :deadline, as: :string, class: "datepicker" %>
		</div>
		<div class="form-group">
			<%= f.input :address %>
		</div>
</form>
</div>
</div>
<div class="col-sm-6">
	<div class="block">
		<form>
			<div class="form-group-2">
				<%= f.input :description %>
			</div>
				<%= f.button :submit %>
			<% end %>
		</form>
	</div>
</div>


